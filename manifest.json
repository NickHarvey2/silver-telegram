{
  "manifest_version": 2,

  "name": "Silver Telegram",
  "description": "Organize your tabs",
  "version": "0.1",

  "browser_action": {
    "default_icon": "silver-telegram.png",
    "default_popup": "silver-telegram.html"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
	"file:///*"
  ]
}